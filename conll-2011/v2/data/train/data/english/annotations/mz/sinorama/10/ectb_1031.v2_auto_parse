                             (NN person))
                         (PP (IN of)
                             (NP (JJ high)
                                 (NNS morals))))
                     (VP (MD will)
                         (ADVP (RB never))
                         (VP (VB be)
                             (ADJP (JJ alone)))))))
        (. .)
        ('' ")))

(TOP (S (NP (PRP We))
        (ADVP (RB now))
        (VP (VBP look)
            (PP (IN with)
                (NP (NP (JJ high)
                        (NNS hopes))
                    (PP (IN to)
                        (NP (DT the)
                            (JJ international)
                            (NN community))))))
        (. .)))

